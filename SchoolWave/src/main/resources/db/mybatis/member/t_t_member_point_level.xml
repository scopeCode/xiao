<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.xcrm.web.member.level">	<resultMap id="MemberPointLevelResultMap" type="com.xcrm.mt.bis.member.vo.MemberPointLevelVo">		<result property="id" column="id" />		<result property="levelName" column="levelName" />	</resultMap>	<select id="queryMemberPointLevel" resultType="com.xcrm.mt.bis.member.vo.MemberPointLevelVo">		SELECT id,levelName		FROM t_t_member_point_level		WHERE tenantId = #{tenantId} AND dataStatus = 1	</select></mapper>